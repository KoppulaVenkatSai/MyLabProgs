   "source": [
    "print(\"Enter any number to convert into binary, Enter '999' to exit.\")\n",
    "while True:\n",
    "    x = int(input(\"Enter the number: \"))\n",
    "    if x == 999:\n",
    "        break\n",
    "    elif x < 0:\n",
    "        print(\"Enter positive numbers\")\n",
    "    else:\n",
    "        print(x,\"in binary:\",bin(x))"
   ]
